#: /opt/tboplayer/tboplayer.py:1342
msgid ""
"To control playing, type a key\n"
"p - pause/play\n"
"spacebar - pause/play\n"
"q - quit\n"
msgstr ""
"Para controlar la reproducción, pulse una tecla\n"
"p - pausa/reproducir\n"
"Espacio - pausa/reproducir\n"
"q - salir\n"

#: /opt/tboplayer/tboplayer.py:1343
msgid ""
"+ - increase volume\n"
"- - decrease volume\n"
"z - tv show info\n"
"1 - reduce speed\n"
"o - forward a chapter\n"
msgstr ""
"+ - incrementa volumen\n"
"- - decrementa volumen\n"
"z - mostrar información de tv\n"
"1 - reduce velocidad\n"
"o - avanzar un capí­tulo\n"

#: /opt/tboplayer/tboplayer.py:1344
msgid ""
"2 - increase speed\n"
"j - previous audio index\n"
"k - next audio index\n"
"i - back a chapter\n"
"n - previous subtitle index\n"
msgstr ""
"2 - incrementa velocidad\n"
"j - í­ndice del audio anterior\n"
"k - í­ndice del audio siguiente\n"
"i - volver un capí­tulo \n"
"n - í­ndice del capí­tulo anterior\n"

#: /opt/tboplayer/tboplayer.py:1345
msgid ""
"m - next subtitle index\n"
"s - toggle subtitles\n"
">cursor - seek forward 30\n"
"<cursor - seek back 30\n"
msgstr ""
"m - í­ndice del subtí­tulo siguiente\n"
"s - intercambia subtí­tulos\n"
">cursor - saltar hacia adelante 30s\n"
"<cursor - saltar hacia atrás 30s\n"

#: /opt/tboplayer/tboplayer.py:1346
msgid ""
"SHIFT >cursor - seek forward 600\n"
"SHIFT <cursor - seek back 600\n"
"CTRL >cursor - next track\n"
"CTRL <cursor - previous track\n"
msgstr ""
"SHIFT >cursor - saltar hacia adelante 600s\n"
"SHIFT <cursor - saltar hacia atrás 600s\n"
"CTRL >cursor - pista siguiente\n"
"CTRL <cursor - pista anterior\n"

#: /opt/tboplayer/tboplayer.py:1347
msgid ""
"F11 - toggle full screen/windowed mode\n"
"\n"
"For more help, consult the 'Operation' section of the README file"
msgstr ""
"F11 - intercambia pantalla completa/pantalla en ventana\n"
"\n"
"Para más ayuda,  consulte la sección 'Operación' del archivo README"

