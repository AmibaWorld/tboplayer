#: /opt/tboplayer/tboplayer.py:1342
msgid ""
"To control playing, type a key\n"
"p - pause/play\n"
"spacebar - pause/play\n"
"q - quit\n"
msgstr ""
"Pour jouer, taper un touche\n"
"p - pause/jouer\n"
"Espace - pause/jouer\n"
"q - sortir\n"

#: /opt/tboplayer/tboplayer.py:1343
msgid ""
"+ - increase volume\n"
"- - decrease volume\n"
"z - tv show info\n"
"1 - reduce speed\n"
"o - forward a chapter\n"
msgstr ""
"+ - augmenter volume\n"
"- - diminuer volumen\n"
"z - montrer informations de tv\n"
"1 - ralentir\n"
"o - chapitre suivant\n"

#: /opt/tboplayer/tboplayer.py:1344
msgid ""
"2 - increase speed\n"
"j - previous audio index\n"
"k - next audio index\n"
"i - back a chapter\n"
"n - previous subtitle index\n"
msgstr ""
"2 - accélerer\n"
"j - piste précédente\n"
"k - piste suivante\n"
"i - chapitre précédent\n"
"n - index sous-titre précédent\n"

#: /opt/tboplayer/tboplayer.py:1345
msgid ""
"m - next subtitle index\n"
"s - toggle subtitles\n"
">cursor - seek forward 30\n"
"<cursor - seek back 30\n"
msgstr ""
"m - index sous-titre suivant\n"
"s - changer les sous-titres\n"
">cursor - avance de 30s\n"
"<cursor - recul de  30s\n"

#: /opt/tboplayer/tboplayer.py:1346
msgid ""
"SHIFT >cursor - seek forward 600\n"
"SHIFT <cursor - seek back 600\n"
"CTRL >cursor - next track\n"
"CTRL <cursor - previous track\n"
msgstr ""
"SHIFT >cursor - avance de 600s\n"
"SHIFT <cursor - recul de 600s\n"
"CTRL >cursor - piste suivante\n"
"CTRL <cursor - piste précédente\n"

#: /opt/tboplayer/tboplayer.py:1347
msgid ""
"F11 - toggle full screen/windowed mode\n"
"\n"
"For more help, consult the 'Operation' section of the README file"
msgstr ""
"F11 - bascule plein écran/fenêtre\n"
"\n"
"Pour plus d'aide, consulter la section 'Operation' dans le fichier README"

